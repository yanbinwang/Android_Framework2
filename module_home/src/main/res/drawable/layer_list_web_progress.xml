<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>

            <corners android:radius="2pt" />

            <gradient
                android:angle="270"
                android:centerColor="@color/bgWebCenter"
                android:endColor="@color/bgWebEnd"
                android:startColor="@color/bgWebStart" />

        </shape>

    </item>

    <item android:id="@android:id/progress">
        <clip>

            <shape>

                <corners android:radius="2pt" />

                <gradient
                    android:centerColor="@color/bgWebProgressCenter"
                    android:endColor="@color/bgWebProgressEnd"
                    android:startColor="@color/bgWebProgressStart" />

            </shape>

        </clip>

    </item>

</layer-list>